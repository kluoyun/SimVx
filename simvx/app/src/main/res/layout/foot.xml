<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_height="match_parent"
	android:layout_width="match_parent"
	android:id="@+id/footRelativeLayout2">

	<RelativeLayout
		android:layout_height="53dp"
		android:layout_width="wrap_content"
		android:orientation="horizontal"
		android:gravity="top|center"
		android:layout_alignBottom="@id/footRelativeLayout2"
		android:layout_alignParentBottom="true"
		android:id="@+id/footRelativeLayout1"
		android:background="#FFFFFF">

		<View
			android:layout_height="0.5dp"
			android:layout_width="match_parent"
			android:background="#000000"
			android:id="@+id/footView1"/>

		<LinearLayout
			android:layout_height="35dp"
			android:layout_width="match_parent"
			android:gravity="bottom|center"
			android:layout_below="@id/footView1"
			android:id="@+id/footLinearLayout1">

			<ImageButton
				android:layout_height="34dp"
				android:layout_width="35dp"
				android:background="@drawable/ic_launcher"
				android:id="@+id/footImageButton1"
				android:onClick="dbdh"/>

			<ImageButton
				android:layout_height="34dp"
				android:layout_width="35dp"
				android:layout_toRightOf="@id/footImageButton1"
				android:layout_marginLeft="50dp"
				android:background="@drawable/ic_launcher"
				android:id="@+id/footImageButton2"
				android:onClick="dbdh"/>

			<ImageButton
				android:layout_height="34dp"
				android:layout_width="35dp"
				android:layout_toRightOf="@id/footImageButton2"
				android:layout_marginLeft="50dp"
				android:background="@drawable/ic_launcher"
				android:id="@+id/footImageButton3"
				android:onClick="dbdh"/>

			<ImageButton
				android:layout_height="34dp"
				android:layout_width="35dp"
				android:layout_toRightOf="@id/footImageButton3"
				android:layout_marginLeft="50dp"
				android:background="@drawable/ic_launcher"
				android:id="@+id/footImageButton4"
				android:onClick="dbdh"/>

		</LinearLayout>

		<LinearLayout
			android:layout_height="17dp"
			android:layout_width="match_parent"
			android:orientation="horizontal"
			android:layout_below="@id/footLinearLayout1"
			android:gravity="top|center">

			<TextView
				android:layout_height="15dp"
				android:layout_width="35dp"
				android:text="首页"
				android:gravity="top|center"
				android:textSize="13sp"
				android:id="@+id/footTextView1"
				android:onClick="dbdh"/>

			<TextView
				android:layout_height="15dp"
				android:layout_width="35dp"
				android:text="精选"
				android:layout_marginLeft="50dp"
				android:gravity="top|center"
				android:textSize="13sp"
				android:id="@+id/footTextView2"
				android:onClick="dbdh"/>

			<TextView
				android:layout_height="15dp"
				android:layout_width="35dp"
				android:text="更多"
				android:layout_marginLeft="50dp"
				android:gravity="top|center"
				android:textSize="13sp"
				android:id="@+id/footTextView3"
				android:onClick="dbdh"/>

			<TextView
				android:layout_height="15dp"
				android:layout_width="35dp"
				android:text="我的"
				android:layout_marginLeft="50dp"
				android:gravity="top|center"
				android:textSize="13sp"
				android:id="@+id/footTextView4"
				android:onClick="dbdh"/>

		</LinearLayout>

	</RelativeLayout>

</RelativeLayout>

